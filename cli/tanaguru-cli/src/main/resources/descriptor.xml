            </includes>
            <fileMode>755</fileMode>
            <outputDirectory>/install-web-app/sql-management</outputDirectory>
        </fileSet>
        <fileSet>
            <directory>${basedir}/../../web-app/tgol-resources/src/main/resources/password-encoder</directory>
            <includes>
                <include>jacksum.jar</include>
                <include>README.txt</include>
                <include>passwd.sh</include>
            </includes>
            <fileMode>755</fileMode>
            <outputDirectory>/install-web-app/sql-management/Passwdmd5</outputDirectory>
        </fileSet>
        <fileSet>
            <directory>${basedir}/../../web-app/tgol-resources/src/main/resources/runtime-lib</directory>
            <includes>
                <include>**/*</include>
            </includes>
            <fileMode>666</fileMode>
            <outputDirectory>/install-web-app/lib</outputDirectory>
        </fileSet>
        <fileSet>
            <directory>${basedir}/../../web-app/tgol-resources/src/main/resources/token-master-key-encryptor</directory>
            <includes>
                <include>**/*</include>
            </includes>
            <fileMode>755</fileMode>
            <outputDirectory>/install-web-app/token-master-key-encryptor</outputDirectory>
        </fileSet>
        <fileSet>
            <directory>${basedir}/../../web-app/geshi</directory>
            <includes>
                <include>**/*</include>
            </includes>
            <fileMode>666</fileMode>
            <outputDirectory>/install-web-app/geshi</outputDirectory>
        </fileSet>
<!--        <fileSet>
            <directory>${basedir}/../../web-app/tgol-web-app/target</directory>
            <includes>
                <include>*.war</include>
            </includes>
            <fileMode>666</fileMode>
            <outputDirectory>/install-web-app/</outputDirectory>
        </fileSet> -->
    </fileSets>
</assembly>
