            </includes>
            <fileMode>755</fileMode>
            <outputDirectory>/update/sql-update</outputDirectory>
        </fileSet>
        <fileSet>
            <directory>${basedir}/../../web-app/tgol-resources/src/main/resources/sql</directory>
            <includes>
                <include>tgol-20-create-tables.sql</include>
                <include>tgol-30-insert.sql</include>
            </includes>
            <fileMode>755</fileMode>
            <outputDirectory>/install-web-app/sql</outputDirectory>
        </fileSet>
        <fileSet>
            <directory>${basedir}/../../web-app/tgol-resources/src/main/resources/conf</directory>
            <includes>
                <include>ESAPI.properties</include>
                <include>google-code.properties</include>
                <include>sign-up-mail.properties</include>
                <include>tgol.conf</include>
            </includes>
            <fileMode>666</fileMode>
            <outputDirectory>/install-web-app/conf</outputDirectory>
        </fileSet>
        <fileSet>
            <directory>${basedir}/src/main/resources/conf/context</directory>
            <includes>
                <include>language-detection.conf</include>
            </includes>
            <fileMode>666</fileMode>
            <outputDirectory>/install-web-app/conf</outputDirectory>
        </fileSet>
        <fileSet>
