<assembly xmlns="http://maven.apache.org/plugins/maven-resources-plugin/resources/1.1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/plugins/maven-resources-plugin/resources/1.1.0 http://maven.apache.org/xsd/resources-1.1.0.xsd">
    <formats>
        <format>tar.gz</format>
    </formats>
    <dependencySets>
        <dependencySet>
            <outputDirectory>/lib</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
        </dependencySet>
    </dependencySets>
    <fileSets>
        <fileSet>
            <directory>${basedir}/src/main/resources/</directory>
            <includes>
                <include>README</include>
            </includes>
            <fileMode>755</fileMode>
            <outputDirectory>/</outputDirectory>
        </fileSet>
        <fileSet>
            <directory>${basedir}/src/main/resources/bin</directory>
            <includes>
                <include>*.sh</include>
            </includes>
            <fileMode>755</fileMode>
            <outputDirectory>/bin</outputDirectory>
        </fileSet>
        <fileSet>
            <directory>${basedir}/src/main/resources/conf/context</directory>
            <includes>
                <include>*.xml</include>
                <include>tanaguru.conf</include>
                <include>**/*</include>
            </includes>
            <fileMode>666</fileMode>
            <outputDirectory>/conf/context</outputDirectory>
        </fileSet>
        <fileSet>
            <directory>${basedir}/../../engine/tanaguru-resources/src/main/resources/conf/context/crawler</directory>
            <includes>
                <include>*.xml</include>
            </includes>
            <fileMode>666</fileMode>
            <outputDirectory>/conf/context/crawler</outputDirectory>
