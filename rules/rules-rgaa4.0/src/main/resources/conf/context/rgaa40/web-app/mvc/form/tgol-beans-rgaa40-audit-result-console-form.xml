    <bean id="rgaa40ThemeSelectFormFieldBuilder" class="org.asqatasun.webapp.form.builder.SelectFormFieldBuilderImpl">
        <property name="i18nKey" value="result.theme"/>
        <property name="errorI18nKey" value="result.theme-error"/>
        <property name="value" value="Rgaa40"/>
        <property name="selectElementBuilderMap">
            <map>
                <entry key="theme">
                    <list>
                        <ref local="rgaa40AllThemeSelectElementBuilder"/>
                                                <ref local="rgaa40-1ThemeSelectElementBuilder"/>
                                                <ref local="rgaa40-2ThemeSelectElementBuilder"/>
                                                <ref local="rgaa40-3ThemeSelectElementBuilder"/>
                                                <ref local="rgaa40-4ThemeSelectElementBuilder"/>
                                                <ref local="rgaa40-5ThemeSelectElementBuilder"/>
