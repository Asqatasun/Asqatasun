<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <!-- audit factories beans -->
    <bean id="auditFactory" class="org.tanaguru.entity.factory.audit.AuditFactoryImpl"/>
    <bean id="contentFactory" class="org.tanaguru.entity.factory.audit.ContentFactoryImpl"/>
    <bean id="definiteResultFactory" class="org.tanaguru.entity.factory.audit.DefiniteResultFactoryImpl"/>
    <bean id="evidenceElementFactory" class="org.tanaguru.entity.factory.audit.EvidenceElementFactoryImpl"/>
    <bean id="evidenceFactory" class="org.tanaguru.entity.factory.audit.EvidenceFactoryImpl"/>
    <bean id="indefiniteResultFactory" class="org.tanaguru.entity.factory.audit.IndefiniteResultFactoryImpl"/>
    <bean id="processRemarkFactory" class="org.tanaguru.entity.factory.audit.ProcessRemarkFactoryImpl"/>
    <bean id="processResultFactory" class="org.tanaguru.entity.factory.audit.ProcessResultFactoryImpl"/>
    <bean id="preProcessResultFactory" class="org.tanaguru.entity.factory.audit.PreProcessResultFactoryImpl"/>
    <bean id="sourceCodeRemarkFactory" class="org.tanaguru.entity.factory.audit.SourceCodeRemarkFactoryImpl"/>
    <bean id="webResourceFactory" class="org.tanaguru.entity.factory.subject.WebResourceFactoryImpl"/>
<!--    <bean id="snapshotFactory" class="org.tanaguru.entity.factory.subject.SnapshotFactoryImpl"/>-->

    <!-- reference factories beans -->
    <bean id="levelFactory" class="org.tanaguru.entity.factory.reference.LevelFactoryImpl"/>
    <bean id="nomenclatureFactory" class="org.tanaguru.entity.factory.reference.NomenclatureFactoryImpl"/>
    <bean id="referenceFactory" class="org.tanaguru.entity.factory.reference.ReferenceFactoryImpl"/>
    <bean id="scopeFactory" class="org.tanaguru.entity.factory.reference.ScopeFactoryImpl"/>
    <bean id="testFactory" class="org.tanaguru.entity.factory.reference.TestFactoryImpl"/>
    <bean id="themeFactory" class="org.tanaguru.entity.factory.reference.ThemeFactoryImpl"/>
    <bean id="criterionFactory" class="org.tanaguru.entity.factory.reference.CriterionFactoryImpl"/>

    <!-- parameter factories beans -->
    <bean id="parameterFactory" class="org.tanaguru.entity.factory.parameterization.ParameterFactoryImpl"/>
    <bean id="parameterElementFactory" class="org.tanaguru.entity.factory.parameterization.ParameterElementFactoryImpl"/>
    <bean id="parameterFamilyFactory" class="org.tanaguru.entity.factory.parameterization.ParameterFamilyFactoryImpl"/>

    <!-- statistics factories beans -->
    <bean id="testStatisticsFactory" class="org.tanaguru.entity.factory.statistics.TestStatisticsFactoryImpl"/>
    <bean id="themeStatisticsFactory" class="org.tanaguru.entity.factory.statistics.ThemeStatisticsFactoryImpl"/>
    <bean id="webResourceStatisticsFactory" class="org.tanaguru.entity.factory.statistics.WebResourceStatisticsFactoryImpl"/>
    <bean id="criterionStatisticsFactory" class="org.tanaguru.entity.factory.statistics.CriterionStatisticsFactoryImpl"/>

</beans>
