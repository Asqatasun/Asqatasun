            <artifactId>asqatasun-crawler</artifactId>
            <scope>runtime</scope>
        </dependency>
        <dependency>
            <groupId>${asqatasunGroupId}</groupId>
            <artifactId>asqatasun-contentloader</artifactId>
            <scope>runtime</scope>
        </dependency>
        <dependency>
            <groupId>${asqatasunGroupId}</groupId>
